{
  "name": "hosts-helper",
	"displayName": "Hosts Editing Helper",
	"description": "A helper for editing hosts files.",
	"publisher": "lanyue",
	"version": "0.1.0",
	"main": "./out/extension.js",
	"engines": {
		"vscode": "^1.72.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onView:hosts-tree-view",
    "onCommand:hosts-helper.helloWorld"
	],
	"github": "https://github.com/mengdu/vscode-database-bench",
	"repository": {
		"type": "https",
		"url": "git+https://github.com/mengdu/vscode-database-bench"
	},
	"license": "MIT",
	"contributes": {
		"views": {
			"explorer": [
				{
					"id": "hosts-tree-view",
					"name": "Hosts Helper",
					"icon": "$(server)"
				}
			]
		},
		"commands": [
			{
				"command": "hosts-helper.helloWorld",
				"title": "Hello World"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.72.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"eslint": "^7.27.0",
		"@typescript-eslint/eslint-plugin": "^4.26.0",
		"@typescript-eslint/parser": "^4.26.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2"
	}
}
